<mujoco model="usb_insertion">
  <compiler angle="radian" autolimits="true"/>
  
  <option gravity="0 0 -9.81" timestep="0.002">
    <flag warmstart="enable"/>
  </option>
  
  <default>
    <joint damping="0.5" armature="0.01"/>
    <geom friction="1.0 0.5 0.5" condim="4" contype="1" conaffinity="1"/>
    <position kp="20" kv="2"/>
  </default>
  
  <asset>
    <texture type="skybox" builtin="gradient" rgb1="0.4 0.5 0.6" rgb2="0.1 0.1 0.15" width="512" height="512"/>
    <texture name="wood" type="2d" builtin="flat" rgb1="0.4 0.25 0.13" rgb2="0.35 0.2 0.1" width="512" height="512"/>
    <texture name="grid" type="2d" builtin="checker" rgb1="0.15 0.15 0.18" rgb2="0.2 0.2 0.23" width="512" height="512"/>
    
    <material name="table" texture="wood" texrepeat="4 4"/>
    <material name="floor" texture="grid" texrepeat="10 10"/>
    <material name="gripper_metal" rgba="0.4 0.4 0.45 1"/>
    <material name="finger_rubber" rgba="0.2 0.2 0.2 1"/>
    <material name="finger_tip" rgba="0.1 0.1 0.1 1"/>
    <material name="usb_body" rgba="0.1 0.1 0.12 1"/>
    <material name="usb_metal" rgba="0.7 0.7 0.75 1"/>
    <material name="port_plastic" rgba="0.15 0.15 0.2 1"/>
    <material name="port_inner" rgba="0.05 0.05 0.05 1"/>
  </asset>
  
  <worldbody>
    <!-- Ground and table -->
    <geom name="floor" type="plane" size="1 1 0.1" material="floor" pos="0 0 0"/>
    <geom name="table" type="box" size="0.4 0.3 0.02" pos="0 0 0.15" material="table"/>
    
    <!-- USB Port (fixed) -->
    <body name="usb_port" pos="0.15 0 0.2">
      <geom name="port_housing" type="box" size="0.03 0.04 0.02" material="port_plastic"/>
      <!-- The slot where USB goes -->
      <geom name="port_slot_back" type="box" size="0.008 0.02 0.004" pos="-0.015 0 0" material="port_inner" contype="1" conaffinity="1"/>
      <geom name="port_slot_top" type="box" size="0.015 0.018 0.001" pos="0 0 0.005" material="port_inner"/>
      <geom name="port_slot_bottom" type="box" size="0.015 0.018 0.001" pos="0 0 -0.005" material="port_inner"/>
      <site name="port_target" pos="0 0 0" size="0.01" rgba="0 1 0 0.3"/>
    </body>
    
    <!-- USB Stick (manipulatable) -->
    <body name="usb_stick" pos="-0.05 0 0.19">
      <freejoint name="usb_joint"/>
      <!-- Main body -->
      <geom name="usb_body" type="box" size="0.025 0.012 0.006" pos="-0.015 0 0" material="usb_body" mass="0.02"/>
      <!-- Metal connector -->
      <geom name="usb_connector" type="box" size="0.012 0.008 0.003" pos="0.012 0 0" material="usb_metal" mass="0.005"/>
      <site name="usb_tip" pos="0.024 0 0" size="0.003" rgba="1 0 0 0.5"/>
    </body>
    
    <!-- Robotiq-style 3-Finger Gripper -->
    <body name="gripper_base" pos="0 0 0.35">
      <joint name="base_x" type="slide" axis="1 0 0" range="-0.3 0.3" damping="5"/>
      <joint name="base_y" type="slide" axis="0 1 0" range="-0.3 0.3" damping="5"/>
      <joint name="base_z" type="slide" axis="0 0 1" range="0.15 0.5" damping="5"/>
      <joint name="base_roll" type="hinge" axis="1 0 0" range="-3.14 3.14" damping="1"/>
      <joint name="base_pitch" type="hinge" axis="0 1 0" range="-1.57 1.57" damping="1"/>
      <joint name="base_yaw" type="hinge" axis="0 0 1" range="-3.14 3.14" damping="1"/>
      
      <geom name="palm" type="cylinder" size="0.04 0.02" material="gripper_metal"/>
      
      <!-- Finger A -->
      <body name="finger_a_proximal" pos="0.035 0 -0.02">
        <joint name="finger_a_base" type="hinge" axis="0 1 0" range="-0.2 1.4"/>
        <geom name="finger_a_prox" type="capsule" fromto="0 0 0 0.04 0 -0.03" size="0.012" material="finger_rubber"/>
        <body name="finger_a_distal" pos="0.04 0 -0.03">
          <joint name="finger_a_tip" type="hinge" axis="0 1 0" range="-0.4 1.2"/>
          <geom name="finger_a_dist" type="capsule" fromto="0 0 0 0.03 0 -0.025" size="0.01" material="finger_tip"/>
          <site name="finger_a_site" pos="0.03 0 -0.025" size="0.005"/>
        </body>
      </body>
      
      <!-- Finger B -->
      <body name="finger_b_proximal" pos="-0.0175 0.0303 -0.02" euler="0 0 2.094">
        <joint name="finger_b_base" type="hinge" axis="0 1 0" range="-0.2 1.4"/>
        <geom name="finger_b_prox" type="capsule" fromto="0 0 0 0.04 0 -0.03" size="0.012" material="finger_rubber"/>
        <body name="finger_b_distal" pos="0.04 0 -0.03">
          <joint name="finger_b_tip" type="hinge" axis="0 1 0" range="-0.4 1.2"/>
          <geom name="finger_b_dist" type="capsule" fromto="0 0 0 0.03 0 -0.025" size="0.01" material="finger_tip"/>
          <site name="finger_b_site" pos="0.03 0 -0.025" size="0.005"/>
        </body>
      </body>
      
      <!-- Finger C -->
      <body name="finger_c_proximal" pos="-0.0175 -0.0303 -0.02" euler="0 0 -2.094">
        <joint name="finger_c_base" type="hinge" axis="0 1 0" range="-0.2 1.4"/>
        <geom name="finger_c_prox" type="capsule" fromto="0 0 0 0.04 0 -0.03" size="0.012" material="finger_rubber"/>
        <body name="finger_c_distal" pos="0.04 0 -0.03">
          <joint name="finger_c_tip" type="hinge" axis="0 1 0" range="-0.4 1.2"/>
          <geom name="finger_c_dist" type="capsule" fromto="0 0 0 0.03 0 -0.025" size="0.01" material="finger_tip"/>
          <site name="finger_c_site" pos="0.03 0 -0.025" size="0.005"/>
        </body>
      </body>
    </body>
  </worldbody>
  
  <actuator>
    <position name="act_base_x" joint="base_x" kp="100" ctrlrange="-0.3 0.3"/>
    <position name="act_base_y" joint="base_y" kp="100" ctrlrange="-0.3 0.3"/>
    <position name="act_base_z" joint="base_z" kp="100" ctrlrange="0.15 0.5"/>
    <position name="act_base_roll" joint="base_roll" kp="50" ctrlrange="-3.14 3.14"/>
    <position name="act_base_pitch" joint="base_pitch" kp="50" ctrlrange="-1.57 1.57"/>
    <position name="act_base_yaw" joint="base_yaw" kp="50" ctrlrange="-3.14 3.14"/>
    <position name="act_finger_a_base" joint="finger_a_base" kp="10" ctrlrange="-0.2 1.4"/>
    <position name="act_finger_a_tip" joint="finger_a_tip" kp="5" ctrlrange="-0.4 1.2"/>
    <position name="act_finger_b_base" joint="finger_b_base" kp="10" ctrlrange="-0.2 1.4"/>
    <position name="act_finger_b_tip" joint="finger_b_tip" kp="5" ctrlrange="-0.4 1.2"/>
    <position name="act_finger_c_base" joint="finger_c_base" kp="10" ctrlrange="-0.2 1.4"/>
    <position name="act_finger_c_tip" joint="finger_c_tip" kp="5" ctrlrange="-0.4 1.2"/>
  </actuator>
  
  <sensor>
    <touch name="finger_a_touch" site="finger_a_site"/>
    <touch name="finger_b_touch" site="finger_b_site"/>
    <touch name="finger_c_touch" site="finger_c_site"/>
  </sensor>
</mujoco>
